{
  "expo": {
    "name": "Medicare",
    "slug": "medicare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "medicalbookingapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/Medicare-first.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.uuofi.MedicalBookingApp",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "نحتاج الوصول إلى الصور لتحديث ملفك الشخصي.",
        "NSPhotoLibraryAddUsageDescription": "يتم حفظ الصور في المعرض عند الحاجة.",
        "NSCameraUsageDescription": "نستخدم الكاميرا لالتقاط صور الملف الشخصي إذا رغبت بذلك.",
        "NSLocationWhenInUseUsageDescription": "نستخدم موقعك أثناء استخدام التطبيق لاختيار موقع العيادة على الخريطة.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.uuofi.MedicalBookingApp",
      "googleServicesFile": "./google-services.json",
      "edgeToEdgeEnabled": true,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#000000"
      },
      "permissions": [
        "CAMERA",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_MEDIA_IMAGES",
        "READ_EXTERNAL_STORAGE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "output": "single"
    },
    "plugins": [
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Medicare-first.png",
          "resizeMode": "cover",
          "backgroundColor": "#000000"
        }
      ]
    ],
    "extra": {
      "apiBaseUrl": "https://api.medicare-iq.com",
      "privacyPolicyUrl": "https://medicare-iq.com/privacy",
      "accountDeletionUrl": "",
      "eas": {
        "projectId": "a121585f-68c7-4b75-b248-37eede596013"
      }
    },
    "owner": "uuofimedicares-organization"
  }
}
